{
  "name": "phpflo/phpflo-flowtrace",
  "description": "Log phpflo network execution for debugging and analysis",
  "type": "library",
  "keywords": ["fbp", "flow based programming", "log"],
  "license": "MIT",
  "authors": [
    {
      "name": "Marc Aschmann",
      "email": "maschmann@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "<5.0",
    "mikey179/vfsStream": "^1.6",
    "phpflo/phpflo": "^1.0"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "PhpFlo\\": "lib/PhpFlo/"
    }
  },
  "autoload-dev": {
    "psr-4": { "Tests\\": "tests/" }
  }
}
